    </PreferenceScreen><PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" android:title="Leistungsprofil" android:summary="Hier sind einzelne Daten deiner bisherigen Leistungen einsehbar." android:key="performance_profile" android:shouldDisableView="true" android:enabled="false" android:selectable="false">
        <EditTextPreference android:summary="Hier findet du alle deine gelaufenen Kilometer." android:key="performance_km" android:title="bisher gelaufene Kilometer" android:defaultValue="0"/>
        <EditTextPreference android:summary="Hier findet du alle bisher verbrannten Kalorien (Höhenmeter einbezogen)" android:key="performance_kalorien" android:title="bisher verbrannte Kalorien" android:defaultValue="0"/>
        <EditTextPreference android:summary="Hier findet du deine bisher gelaufene Zeit." android:key="performance_time" android:title="gelaufene Zeit" android:defaultValue="0"/>
        <EditTextPreference android:summary="Hier findest du die Anzahl an Trainings die du absolviert hast." android:title="Anzahl von Trainings" android:defaultValue="0" android:key="performance_training"/>
        <EditTextPreference android:summary="Hier findest du die Anzahl der bisherigen Challenges." android:key="performance_challenge" android:title="Anzahl der Challenges" android:defaultValue="0"/>
