            <Preference android:title="Läufer auswählen" android:summary="@string/help_display_runner_chance_txt" />
            <Preference android:title="Achtung" android:summary="@string/help_display_attantion_txt" />
            <Preference android:title="Hinweis" android:summary="@string/help_display_tip_txt" />
            <Preference android:title="GeoPlayer" android:summary="@string/help_display_geoplayer_txt" />
            <Preference android:title="Achtung" android:summary="@string/help_geoplayer_attantion_txt" />
            <Preference android:title="laden von GeoPlayer Listen" android:summary="@string/help_display_load_geoplayerlist_txt" />
