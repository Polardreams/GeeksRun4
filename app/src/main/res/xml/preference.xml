<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" android:title="Erweiterte Einstellungen">
    <PreferenceScreen android:title="Personalien" android:key="pers" android:summary="Alles rund um deine Kondition und Person">
        <EditTextPreference android:summary="Gebe deinen Namen ein." android:title="Name" android:key="pers_name" android:defaultValue="Runner"/>
        <EditTextPreference android:summary="Gebe dein Alter ein." android:title="Alter" android:key="pers_age" android:defaultValue="25"/>
        <EditTextPreference android:title="Größe" android:key="pers_height" android:summary="Gebe deine Körpergröße an." android:defaultValue="186"/>
        <EditTextPreference android:key="pers_weight" android:summary="Gebe dein Körpergewicht an." android:title="Gewicht" android:defaultValue="95"/>
        <ListPreference android:title="Geschlecht" android:key="pers_sexx" android:summary="Gebe dein biologisches Geschlecht an." android:entries="@array/pers_sex" android:entryValues="@array/pers_sex_id"/>
    </PreferenceScreen>
    <PreferenceScreen android:key="hud" android:title="Displayeinstellungen" android:summary="Für deinen optimalen Lauf">
        <CheckBoxPreference android:title="Ansagen" android:summary="Möchtes du Trainingsansagen während des Laufes hören?" android:key="hud_speak" android:defaultValue="true"/>
        <EditTextPreference android:summary="Achtung, das sollte nur ein erfahrener GeeksRunner optimieren!" android:title="GPS-Setzung" android:key="hud_gps_points" android:defaultValue="5000"/>
        <EditTextPreference android:summary="Wie stark vergrößert soll die Karte während des Laufes sein?" android:title="GoogleMap Zoom" android:key="hud_map_zoom" android:defaultValue="15"/>
        <EditTextPreference android:summary="Gebe deine Vorbereitungszeit an." android:title="Countdown" android:key="hud_time_countdown" android:defaultValue="15"/>
        <CheckBoxPreference android:summary="Höhenmeter werden mit in die Verbrennungsrechnung aufgenommen." android:title="Höhenmeter" android:key="hud_height_energie" android:defaultValue="true"/>
        <EditTextPreference android:key="hud_speak_intervall" android:defaultValue="300" android:title="Sprechintervall" android:summary="Aller wieviel Sekunden möchtest du einen Bericht?"/>
        <ListPreference android:title="GPS Ortungsoption" android:key="hud_gps_option" android:summary="Möchtest Du mit oder ohne Wifi Ortung laufen?" android:entries="@array/yes_no" android:entryValues="@array/yes_no"/>
    </PreferenceScreen>
    <PreferenceScreen android:key="help" android:summary="Erfahre mehr über deine App" android:title="Hilfe">
